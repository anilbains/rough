<h1 class="text-center">Create Group </center> </h1>
 
<%= form_for @group do |f| %>
  <div class="form-group">
  
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <br>

  <%= f.label :year %>
  <%= f.text_field :year, class: "form-control" %>
  <br>
  <%= f.file_field :groupphoto %>

  <%= f.submit "Update Group", class: 'btn btn-success btn-lg btn-block' %>
  </div>
<% end %>
