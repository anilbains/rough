<h1 class="text-center">Create Group </center> </h1>
<div class="row">
  <div class="col-sm-6 col-sm-offset-3">
    <%= form_for @group, :html => {:multipart => true} do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: "form-control" %>
        <br>

        <%= f.label :year %>
        <%= f.text_field :year, class: "form-control" %>
        <br>

        <%= f.file_field :groupphoto %>

        <%= f.submit "Create Group", class: 'btn btn-success btn-lg btn-block' %>
      </div>
  </div>
</div>
<% end %>
